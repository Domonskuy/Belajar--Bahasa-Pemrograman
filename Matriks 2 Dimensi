#include <iostream>
using namespace std;

int main(){
	 
	 // Deklarasi Array 2 Dimensi 
	 int matriks[3][2] = {{1,2},{3,4},{5,6}};
	 
	 //Deklarasi variabel perulangan 
	 int i,j;
	 
	 cout<<"\t=========================";
	 cout<<"\n\t=== Tampil Matriks ordo 3X2  ===\n";
	 cout<<"\t=========================";
	 
	 //Menampilkan matriks
	 cout <<"\n Menampilkan Matriks : \n|";
	 for(i = 0; i < 3; i++){
	 	for(j = 0; j < 2; j++){
	 		cout << matriks[i][j] << " ";
		 }
		 cout<< endl;
	 }
	 return 0;
}

